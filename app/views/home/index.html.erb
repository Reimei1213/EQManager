<% if logged_in? %>
  <%= render 'home/logged_in' %>
<% else %>
  <%= render 'home/not_logged_in' %>
<% end %>

<!--<p><%#= @param %></p>-->

<table>
  <thead>
    <tr>
      <th>ユーザー名</th>
      <th>機材名</th>
      <th>借りた日</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @equips then %>
      <% @equips.each do |equip| %>
        <tr>
          <td></td>
          <td><%= equip.name %></td>
          <td></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
