<h1>ログイン</h1>
<%= form_for(:session, url:login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>

<%= link_to '新規登録', new_user_path %>